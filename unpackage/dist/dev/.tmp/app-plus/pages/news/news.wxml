<view class="content"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><listcell class="row" vue-id="{{'1-'+index}}" last="{{true}}" data-event-opts="{{[['^click',[['detail']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="news_left"><image class="user_img" src="{{'/static/img/'+(index+1)+'.jpg'}}" mode="widthFix"></image><view class="news_info"><view class="name">{{item.name}}</view><view class="content">{{item.msg}}</view></view></view><view class="{{['news_right',item.level==3?'dot':'']}}"><view class="time">{{item.time}}</view><block wx:if="{{item.msgNum>0}}"><tui-badge vue-id="{{('2-'+index)+','+('1-'+index)}}" type="{{item.level==2?'gray':'danger'}}" dot="{{item.level==3?true:false}}" bind:__l="__l" vue-slots="{{['default']}}">{{item.level!=3?item.msgNum:""}}</tui-badge></block></view></listcell></block></block></view>